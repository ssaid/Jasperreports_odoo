	<queryString>
		<![CDATA[select

/*Datos compania*/

res_company.name as "company_name",
res_company.email as "company_email",
res_company.phone as "company_phone",
pc.vat as "company_cuit",
pc.website as "company_website",
pc.street as "company_street",

/*Datos picking*/
sp.id as "id",
sp.name as "n_remito",
sp.date_done as "fecha_efectiva",
sp.note as "observaciones",
sp.weight as "peso",
sp.wsremcarne_cre as "cre",
sp.wsremcarne_date_due as "cre_vto",
sp.commercial_zone as "zona",

/*Datos cliente*/
res_partner.name as "nombre_cliente",
res_partner.street as "direccion_cliente",
res_partner.vat as "cuit_cliente",
res_partner.ref as "id_cliente",
account_payment_term.name as "condicion_venta",
res_partner.phone as "tel_cliente",
CASE
	WHEN (account_fiscal_position.name = 'RI' )
    	 THEN 'RESPONSABLE INSCRIPTO'
     WHEN (account_fiscal_position.name = 'RIM' )
     	THEN 'RESPONSABLE INSCRIPTO "M"'
     ELSE
     	account_fiscal_position.name
 END  "cond_iva",

/*Datos carrier*/
cr.name as "nombre_carrier",
cr.street as "direccion_carrier",
cr.vat as "cuit_carrier",

/*Datos chofer*/
dd.name as "chofer_nombre",
drdt.name as "chofer_tipo_doc",
dd.vat as "chofer_doc",


/*Datos camion y trailer*/
tt.name as "patente",
tr.name as "patente_acoplado",

/*descripcion factura*/
res_currency.name as "moneda_nombre",
res_currency.symbol as "moneda",
account_invoice.date_due as "vencimiento_factura",
account_invoice.amount_total as "monto_factura",
res_currency.name as "moneda_nombre",
res_currency.symbol as "moneda",
account_invoice.date_due as "vencimiento_factura",


/*datos productos*/
sm.product_uom_qty as "producto_cant",
sm.name as "producto_descripcion",
uom.name as "u_medida",
sm.boxes_qty as "cant_cajas",
mg.code as "afip_code",



/*imagen */
iat.store_fname AS "Company_image",
qr.value as "qr_path",
icp.value AS "Filestore_path"



/*JOINS*/
from stock_picking sp
left join res_company on res_company.id=sp.company_id
left join res_partner pc on pc.id=res_company.partner_id
left join res_partner on res_partner.id=sp.partner_id
left join account_invoice on account_invoice.origin = sp.origin
left join account_payment_term on account_payment_term.id = account_invoice.payment_term_id
left join account_fiscal_position on account_fiscal_position.id=account_invoice.fiscal_position_id
left join res_currency on account_invoice.currency_id=res_currency.id
left join stock_move sm on sm.picking_id=sp.id
left join uom_uom uom on sm.product_uom = uom.id
left join res_partner cr on sp.carrier_id = cr.id
left join delivery_route rt on sp.route_id = rt.id
left join truck_scale_entry ts on ts.id = rt.scale_entry_id
left join truck_truck tt on tt.id = ts.truck_id
left join truck_trailer tr on tr.id = ts.trailer_id
left join delivery_driver dd on dd.id = rt.driver_id
left join res_document_type drdt on drdt.id = dd.document_type_id
left join product_product pp on pp.id = sm.product_id
left join stock_picking_meat_group mg on mg.id  = pp.meat_type_id

LEFT JOIN ir_attachment iat ON iat.res_id = pc.id AND iat.res_model = 'res.partner' AND iat.res_field = 'image'
LEFT JOIN ir_config_parameter icp ON icp.key = 'filestore.path'

LEFT JOIN ir_config_parameter qr ON qr.key = 'jasper_path_for_qr'

WHERE $X{IN, account_invoice.id, IDS};]]>
