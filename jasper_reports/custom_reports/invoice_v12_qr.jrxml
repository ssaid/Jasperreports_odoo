	<queryString>
		<![CDATA[WITH totals AS (
SELECT
ai.id as invoice_id,
SUM (ait.amount)as sum_amount

FROM account_invoice ai
JOIN account_invoice_tax ait
	ON ai.id = ait.invoice_id
JOIN account_tax at
	ON ait.tax_id = at.id

WHERE at.tax_group = 'vat'
AND ait.name ILIKE  '%iva%'

GROUP BY ai.id
)

SELECT

/*Datos compania*/

rc.name AS "company_name",
COALESCE(rc.email, '') AS "company_email",
COALESCE(rc.phone, '') AS "company_phone",
rpc.vat AS "company_cuit",
COALESCE(rpc.website,'') AS "company_website",
rpc.street AS "company_street",
posar.activity_start_date AS "company_start_date",
icp.value AS "filestore_path",
iat.store_fname AS "company_image",

/*IVA */
totals.sum_amount AS "iva_amount",

/*Datos cliente*/
rp.name AS "customer_name",
rp.street AS "customer_address",
rp.vat AS "customer_cuit",
za.rp_za_account AS "customer_zone_account",
apt.name AS "customer_condition",
rp.phone AS "customer_phone",
CASE
	WHEN (afp.name = 'RI' ) THEN 'RESPONSABLE INSCRIPTO'
	WHEN (afp.name = 'RIM' ) THEN 'RESPONSABLE INSCRIPTO "M"'
	ELSE afp.name
END AS "customer_fiscal_position",

/*Descripcion factura*/
ail.price_unit AS "product_price_unit",
CASE
	WHEN iden.name LIKE 'B' AND at.price_include IS FALSE THEN (ail.price_unit *(1+at.amount/100))
	WHEN iden.name LIKE 'A' AND at.price_include IS TRUE THEN (ail.price_unit / (1+at.amount/100))
	ELSE ail.price_unit
END AS "product_price_unit",
ail.quantity AS "product_qty",
ail.name AS "product_name",
uom.name AS "product_uom",
CASE
	WHEN iden.name LIKE 'B' THEN ail.price_total
	ELSE ail.price_subtotal
END AS "product_price_subtotal",
rcurr.name AS "currency_name",
rcurr.symbol AS "currency_code",

/*Datos factura general*/
ai.id AS "invoice_id",
sp.name AS "picking_number",
ai.date_invoice AS "invoice_date",
ai.date_due AS "invoice_date_due",
ai.internal_number AS "invoice_number",
ai.comment AS "invoice_comments",
ai.amount_untaxed AS "invoice_amount_untaxed" ,
ai.amount_total AS "invoice_total",
ai.amount_tax AS "invoice_tax",
iden.name AS "invoice_denomination",
ai.cae AS "invoice_cae",
ai.cae_due_date AS "invoice_cae_due_date",
vt.name AS "invoice_type",
LPAD(vt.code::text, 3, '0') AS "invoice_voucher_type",
ptl.name AS "perception_name",
ptl.amount AS "perception_amount",
pp.code AS "perception_code",
ROUND(ai.currency_rate, 4) AS "invoice_currency_rate",
ROUND(totals.sum_amount * ai.currency_rate, 2) AS "vat_total",
CASE
	WHEN ai.amount_total_cur > 0 THEN ENCODE(
	('1' || ai.date_invoice || rpc.vat ||
	posar.name || vt.code || SPLIT_PART(ai.internal_number,'-',2) ||
	(ai.amount_total * 100)::BIGINT || rcurr.name ||
	REPLACE(TRUNC(ai.amount_total / ai.amount_total_cur, 6)::text, '.', '') ||
	rdt.afip_code || rp.vat || 'E' || COALESCE(ai.cae, '00000000000000')
	)::bytea,'base64'
)
	ELSE ENCODE(
	('1' || ai.date_invoice || rpc.vat ||
	posar.name || vt.code || SPLIT_PART(ai.internal_number,'-',2) ||
	(ai.amount_total * 100)::BIGINT || rcurr.name ||
	REPLACE(TRUNC(ai.amount_total, 6)::text, '.', '') ||
	rdt.afip_code || rp.vat || 'E' || COALESCE(ai.cae, '00000000000000')
	)::bytea,'base64'
) END AS "invoice_qr",


/*imagen */
iat.store_fname AS "Company_image",
icp.value AS "Filestore_path"



/*JOINS*/
FROM account_invoice ai
LEFT JOIN account_invoice_line ail
	ON ai.id = ail.invoice_id
LEFT JOIN res_company  rc
	ON rc.id=ai.company_id
LEFT JOIN res_partner rpc
	ON rc.partner_id = rpc.id
LEFT JOIN res_partner rp
	ON ai.partner_id = rp.id
JOIN res_document_type rdt
	ON rp.document_type_id = rdt.id
LEFT JOIN account_invoice_line_tax ailt
	ON ailt.invoice_line_id = ail.id
LEFT JOIN account_tax at
	ON at.id = ailt.tax_id
LEFT JOIN sale_order so
	ON so.name = ai.origin
LEFT JOIN stock_picking sp
	ON sp.origin = ai.origin
LEFT JOIN res_partner_zone_account_line za
	ON rp.id = za.rp_za_partners_id
	AND rp_za_product_type_id = so.product_type
LEFT JOIN account_payment_term apt
	ON ai.payment_term_id = apt.id
LEFT JOIN pos_ar posar
	ON posar.id = ai.pos_ar_id
LEFT JOIN uom_uom uom
	ON ail.uom_id= uom.id
LEFT JOIN account_fiscal_position afp
	ON ai.fiscal_position_id = afp.id
LEFT JOIN invoice_denomination  iden
	ON ai.denomination_id = iden.id
LEFT JOIN wsfe_voucher_type vt
	ON ai.voucher_type_id = vt.id
LEFT JOIN totals
	ON ai.id = totals.invoice_id
LEFT JOIN perception_tax_line ptl
	ON ai.id = ptl.invoice_id
LEFT JOIN perception_perception pp
	ON pp.id = ptl.perception_id
LEFT JOIN res_currency rcurr
	ON ail.currency_id=rcurr.id

LEFT JOIN ir_attachment iat
	ON iat.res_id = rpc.id
	AND iat.res_model = 'res.partner'
	AND iat.res_field = 'image'
LEFT JOIN ir_config_parameter icp
	ON icp.key = 'filestore.path'

LEFT JOIN ir_config_parameter qr
	ON qr.key = 'jasper_path_for_qr'


WHERE $X{IN, ai.id, IDS}]]>
	</queryString>
