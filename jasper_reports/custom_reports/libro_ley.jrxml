	<queryString>
		<![CDATA[WITH totals AS (
SELECT
    psl.slip_id AS payslip,
    SUM(psl.total) FILTER (WHERE src.code IN ('REM') )AS sum_rem,
        SUM(psl.total) FILTER (WHERE src.code IN ('NR') )AS sum_nr,
    SUM(psl.total) FILTER (WHERE src.code LIKE 'RET') AS sum_deductions,
    SUM(psl.total) FILTER (WHERE src.code LIKE 'REM' OR src.code LIKE 'NR' OR src.code LIKE 'RET')::integer AS net

FROM hr_payslip_line psl
    JOIN hr_salary_rule sr ON sr.id = psl.salary_rule_id
    JOIN hr_salary_rule_category src ON src.id = sr.category_id
GROUP BY psl.slip_id
)

SELECT
rpc.name AS "Company_name",
rpc.vat AS "Company_vat",
COALESCE(TRIM(rpc.street),'') || ', ' || COALESCE(TRIM(rccty.name),'') || ' ' || COALESCE(TRIM(rccs.name),'') AS "Company_address",
$P{x_page} AS "Initial_page",
ps.id AS "Payslip_id",
str.id AS "Struct_id",
str.name AS "Struct_name",
TO_CHAR(ps.date_to, 'mm/YYYY') AS "Payslip_period",
emp.id AS "Employee_id",
TRIM(emp.other_id) AS "Employee_legajo",
COALESCE(emp.identification_id, '') AS "Employee_doc",
emp.cuil AS "Employee_cuil",
emp.name AS "Employee_name",
rctry.name AS "Employee_country",
COALESCE(TRIM(rp.street),'') || ', ' || COALESCE(TRIM(rcty.name),'') || ' ' || COALESCE(TRIM(rcs.name),'') AS "Employee_address",
COALESCE(TO_CHAR(emp.birthday, 'dd/MM/YYYY'), '  /  /    ') AS "Employee_birthday",
CASE
	WHEN emp.marital = 'single' THEN 'Soltero/a'
	WHEN emp.marital = 'married'THEN 'Casado/a'
	WHEN emp.marital = 'widower' THEN 'Viudo/a'
	WHEN emp.marital = 'divorced' THEN 'Divorciado/a'
	ELSE ''
END AS "Employee_marital",
TRIM(sec.code) || ' - ' || TRIM(sec.name) AS "Employee_sector",
COALESCE(ct.name, '') AS "Employee_category",
ctr.date_start AS "Employee_real_date_start",
COALESCE(TO_CHAR(ctr.date_end, 'dd/MM/YYYY'), '  /  /    ') AS "Employee_date_end",
ABS(tot.sum_rem) AS "Total_rem",
ABS(tot.sum_nr) AS "Total_nr",
ABS(tot.sum_deductions) AS "Total_deductions",
ABS(tot.net) AS "Total_net",
ctr.wage AS "Total_wage"

FROM hr_payslip ps
JOIN hr_payroll_structure str
	ON ps.struct_id = str.id
JOIN hr_payroll_structure_type strt
	ON str.struct_type_id = strt.id
JOIN hr_contract ctr
	ON ctr.id = ps.contract_id
JOIN hr_employee emp
	ON emp.id = ps.employee_id
LEFT JOIN hr_department sec
	ON emp.sector_id = sec.id
LEFT JOIN res_country rctry
	ON emp.country_id = rctry.id
LEFT JOIN res_partner rp
	ON emp.address_home_id = rp.id
LEFT JOIN res_country_state rcs
	ON rp.state_id = rcs.id
LEFT JOIN res_city rcty
	ON rp.city_id = rcty.id
LEFT JOIN hr_contract_type ct
	ON ctr.type_id = ct.id
JOIN res_company rc
	ON rc.id = ps.company_id
JOIN res_partner rpc
	ON rpc.id = rc.partner_id
LEFT JOIN res_country_state rccs
	ON rpc.state_id = rccs.id
LEFT JOIN res_city rccty
	ON rpc.city_id = rccty.id
JOIN totals tot
	ON tot.payslip = ps.id

WHERE
	ps.state != 'cancel'
	AND strt.name IN ('MENSUAL','MENSUAL(J)','EGRESOS','SAC')
	AND ps.date_from >= TO_DATE($P{x_date_start}, 'YYYY-MM-DD')
	AND ps.date_to <= TO_DATE($P{x_date_end}, 'YYYY-MM-DD')
	AND ps.other_payslip = False
	AND str.id IN (substring(array_to_string(ARRAY[$P{x_struct_ids}], ','),3))

ORDER BY emp.warehouse, ctr.struct_id, sec.code, emp.other_id]]>
	</queryString>
