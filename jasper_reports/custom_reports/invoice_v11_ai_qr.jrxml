	<queryString>
		<![CDATA[WITH translation AS
(
        WITH q1 AS
        (
            SELECT ai.id AS invoice_id, apt.id AS payment_term_id
            FROM account_invoice ai
            LEFT JOIN account_payment_term apt ON apt.id = ai.payment_term_id
        )

        SELECT q1.invoice_id AS invoice_id, src, value, lang
        FROM ir_translation it
        JOIN q1 ON it.name='account.payment.term,name'
        AND res_id=q1.payment_term_id AND lang = 'es_AR'
),

invoice_discount AS
(
SELECT
CASE WHEN iden.name = 'A' OR  ai.company_id = 3 THEN SUM (ail.quantity * ail.price_unit * (-ail.discount / 100) )
	ELSE SUM(ail.quantity * (ail.price_unit * 1.21) * (-ail.discount / 100) )
END AS discount,
       ai.id AS invoice_id

FROM account_invoice ai
JOIN account_invoice_line ail ON ail.invoice_id = ai.id
JOIN invoice_denomination iden ON iden.id = ai.denomination_id

GROUP BY ai.id, iden.name
)

SELECT  rc.name AS "Company_name",
        rc.company_registry AS "Company_registry",
        rpw.street AS "Company_street",
        rpw.zip AS "Company_zip",
        rcsc.name AS "Company_state",
        rctyc.name AS "Company_country",
        ( COALESCE(rpw.street, ' ') || ', ' || COALESCE(rcsc.name, ' ') || ', ' || COALESCE(rctyc.name, ' ') ) AS "Company_address",
        rpw.phone AS "Company_phone",
        rpc.vat AS "Company_cuit",
        rpc.website AS "Company_website",
        rpw.email AS "Company_email",
        afpc.name AS "Company_fiscal_position",
        COALESCE(rpc.nro_insc_iibb, '') AS "Company_nro_iibb",
        posar.activity_start_date AS "Company_start_date",
        COALESCE(iatp.store_fname, iat.store_fname) AS "Company_image",
        icp.value AS "Filestore_path",

        rp.name AS "Partner_name",
        rp.street AS "Partner_street",
        rcs.name AS "Partner_state",
        rcty.name AS "Partner_country",
        ( COALESCE(rp.street || ', '  ,   ' ')  || COALESCE(rcs.name || ', '   ,   ' ') || COALESCE(rcty.name || ', '   ,   ' ') || COALESCE(' - CP ' || rp.zip, ' ') ) AS "Partner_address",
        rp.zip AS "Partner_zip",
        COALESCE(rdt.name, '') AS "Partner_document_type",
        rp.vat AS "Partner_cuit",
        afp.name AS "Partner_fiscal_position",

        ai.id AS "Invoice_id",
        rps.name AS "Invoice_seller",
        CASE WHEN ai.type = 'out_invoice' AND ai.is_debit_note IS FALSE THEN 'Factura'
	   WHEN ai.type = 'out_invoice' AND ai.is_debit_note IS TRUE THEN 'Nota de Débito'
	   ELSE 'Nota de Crédito'
        END AS "Invoice_type",
        iden.name AS "Invoice_denomination",
        LPAD(fevt.code::text , 2 , '0') AS "Invoice_voucher_type",
        COALESCE(ai.internal_number, ' FC Borrador ') AS "Invoice_number",
        rcurr.name AS "Invoice_currency",
        COALESCE(TO_CHAR (ai.date_invoice, 'dd/mm/yyyy'), ' ') AS "Invoice_date",
        COALESCE(TO_CHAR (ai.date_due, 'dd/mm/yyyy'), ' ') AS "Invoice_due_date",
        COALESCE(ai.cae, ' ') AS "Invoice_cae",
        COALESCE(TO_CHAR (ai.cae_due_date, 'dd/mm/yyyy'), ' ') AS "Invoice_cae_due_date",
        ai.name AS "Invoice_ref",
        ai.origin AS "Invoice_origin",
        translation.value AS "Invoice_payment_term",
        ai.amount_untaxed AS "Invoice_net_discount_subtotal",
        (ai.amount_untaxed_no_disc + (-invoice_discount.discount)) AS "Invoice_subtotal",
        ai.amount_total AS "Invoice_total",
        ail.name AS "Invoice_line_description",
        ail.quantity AS "Invoice_line_qty",
        (ail.discount / 100) AS "Invoice_line_discount",
        invoice_discount.discount AS "Invoice_sum_discount",
        CASE WHEN iden.name LIKE 'B' AND at.price_include IS FALSE THEN (ail.price_unit *(1+at.amount/100))
             WHEN iden.name LIKE 'A' AND at.price_include IS TRUE THEN (ail.price_unit / (1+at.amount/100))
             ELSE ail.price_unit
        END AS "Invoice_line_price_unit",
        CASE WHEN iden.name LIKE 'B' THEN 'N/A'
	   ELSE at.name
        END AS "Invoice_line_tax",
        CASE WHEN iden.name LIKE 'B' THEN ail.price_total_no_disc
             ELSE ail.price_subtotal_no_disc
        END AS "Invoice_line_total",
        (rpc.vat || LPAD(fevt.code::text , 3 , '0') || posar.name || ai.cae || TO_CHAR (ai.date_due, 'yyyymmdd') ) AS "Invoice_barcode",
        COALESCE(ai.first_discount_percent, 0) AS "Invoice_first_discount",
        COALESCE(ai.second_discount_percent, 0) AS "Invoice_second_discount"


FROM account_invoice ai
    JOIN account_invoice_line ail ON ail.invoice_id = ai.id
    JOIN invoice_denomination iden ON iden.id = ai.denomination_id
    LEFT JOIN account_payment_term apt ON apt.id = ai.payment_term_id
    LEFT JOIN account_invoice_line_tax ailt ON ailt.invoice_line_id = ail.id
    LEFT JOIN account_tax at ON at.id = ailt.tax_id
    LEFT JOIN wsfe_voucher_type fevt ON fevt.document_type = ai.type AND fevt.denomination_id = ai.denomination_id
    LEFT JOIN pos_ar posar ON posar.id = ai.pos_ar_id
    LEFT JOIN res_currency rcurr ON rcurr.id = ai.currency_id
    LEFT JOIN translation ON translation.invoice_id=ai.id
    JOIN invoice_discount ON invoice_discount.invoice_id = ai.id
    LEFT JOIN res_users ru ON ru.id = ai.user_id
    JOIN res_partner rps ON rps.id = ru.partner_id

    JOIN res_partner rp ON rp.id = ai.partner_id
    LEFT JOIN res_country_state rcs ON rcs.id = rp.state_id
    LEFT JOIN res_country rcty ON rcty.id = rp.country_id
    LEFT JOIN ir_property ip ON ip.name ~ 'property_account_position'
            AND ip.res_id=CONCAT('res.partner,',rp.id) AND ip.company_id=ai.company_id
    LEFT JOIN account_fiscal_position afp ON split_part(ip.value_reference, ',', 2)::int=afp.id
	LEFT JOIN res_document_type rdt ON rdt.id = rp.document_type_id

    JOIN res_company rc ON rc.id = ai.company_id
    JOIN res_partner rpc ON rpc.id = rc.partner_id
    JOIN stock_warehouse sw ON sw.id = posar.shop_id
    LEFT JOIN res_partner rpw ON rpw.id = sw.partner_id
    LEFT JOIN res_country_state rcsc ON rcsc.id = rpw.state_id
    LEFT JOIN res_country rctyc ON rctyc.id = rpw.country_id
    LEFT JOIN ir_property ipc ON ipc.name ~ 'property_account_position'
            AND ipc.res_id=CONCAT('res.partner,',rpc.id) AND ipc.company_id=ai.company_id
    LEFT JOIN account_fiscal_position afpc ON split_part(ipc.value_reference, ',', 2)::int=afpc.id
    LEFT JOIN ir_attachment iat ON iat.res_id = rpc.id AND iat.res_model = 'res.partner' AND iat.res_field = 'image'
    LEFT JOIN ir_attachment iatp ON iatp.res_id = ai.pos_ar_id AND iatp.res_model = 'pos.ar' AND iatp.res_field = 'image'
    LEFT JOIN ir_config_parameter icp ON icp.key = 'filestore.path'


WHERE $X{IN, ai.id, IDS}

ORDER BY ai.internal_number, ail.id]]>
	</queryString>
