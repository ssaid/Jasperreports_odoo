	<queryString>
		<![CDATA[SELECT

/*Datos compania*/
rc.name AS "company_name",
rc.email AS "company_email",
rc.phone AS "company_phone",
rpc.vat AS "company_cuit",
rpc.website AS "company_website",
rpc.street AS "company_street",
icp.value AS "filestore_path",
iat.store_fname AS "company_image",

/*Datos picking*/
sp.id AS "picking_id",
sp.name AS "picking_number",
sp.date_done AS "picking_date",
sp.note AS "picking_note",
COALESCE(sp.weight, 0) AS "picking_weight",
COALESCE(sp.package_qty, 0) AS "picking_package_qty",
COALESCE(sp.bags_qty, 0) AS "picking_bags_qty",
COALESCE(sp.baskets_qty, 0) AS "picking_baskets_qty",
COALESCE(sp.pallets, 0) AS "picking_pallets_qty",
sp.wsremcarne_cod_remito AS "picking_aut_code",
sp.wsremcarne_cre AS "picking_cre",
sp.wsremcarne_date_due AS "picking_cre_due_date",
sp.commercial_zone AS "picking_commercial_zone",
sp.cot AS "picking_cot",

/*Datos cliente*/
rp.name AS "customer_name",
rp.street AS "customer_address",
rp.vat AS "customer_cuit",
za.rp_za_account AS "customer_zone_account",
apt.name AS "customer_condition",
rp.phone AS "customer_phone",
CASE
	WHEN (afp.name = 'RI' ) THEN 'RESPONSABLE INSCRIPTO'
	WHEN (afp.name = 'RIM' ) THEN 'RESPONSABLE INSCRIPTO "M"'
	ELSE afp.name
END AS "customer_fiscal_position",

/*Datos carrier*/
rpcr.name AS "carrier_name",
rpcr.street AS "carrier_address",
rpcr.vat AS "carrier_cuit",

/*Datos chofer*/
dd.name AS "driver_name",
drdt.name AS "driver_document_type",
dd.vat AS "driver_document",


/*Datos camion y trailer*/
tt.name AS "truck_patent",
tr.name AS "truck_trailer_patent",

/*descripcion factura*/
ai.internal_number AS "invoice_number",
rcur.name AS "invoice_currency_name",
rcur.symbol AS "invoice_currency_code",
ai.date_due AS "invoice_due_date",
ai.amount_total AS "invoice_total",

/*datos productos*/
sm.product_uom_qty AS "product_qty",
sm.name AS "product_name",
it.value AS "product_uom",
sm.boxes_qty AS "product_boxes_qty",
mt.code AS "product_afip_code"


/*JOINS*/
FROM stock_picking sp
LEFT JOIN res_company  rc
	ON sp.company_id = rc.id
LEFT JOIN res_partner rpc
	ON rc.partner_id = rpc.id

LEFT JOIN res_partner rp
	ON sp.partner_id = rp.id
LEFT JOIN account_invoice ai
	ON sp.origin = ai.origin
LEFT JOIN account_payment_term apt
	ON ai.payment_term_id = apt.id
LEFT JOIN account_fiscal_position afp
	ON ai.fiscal_position_id = afp.id
LEFT JOIN res_currency  rcur
	ON ai.currency_id=rcur.id
LEFT JOIN res_partner_zone_account_line za
	ON rp.id = za.rp_za_partners_id
	AND rp_za_product_type_id = 2

LEFT JOIN stock_move sm
	ON sp.id = sm.picking_id
LEFT JOIN uom_uom uom
	ON sm.product_uom = uom.id
LEFT JOIN ir_translation it
	ON it.name = 'uom.uom,name'
	AND it.lang = 'es_AR'
	AND uom.name = it.src
LEFT JOIN product_product pp
	ON sm.product_id = pp.id
LEFT JOIN stock_picking_meat_type mt
	ON pp.meat_type_id = mt.id

LEFT JOIN res_partner rpcr
	ON sp.carrier_id = rpcr.id
LEFT JOIN delivery_route rt
	ON sp.route_id = rt.id
LEFT JOIN truck_scale_entry ts
	ON rt.scale_entry_id = ts.id
LEFT JOIN truck_truck tt
	ON ts.truck_id = tt.id
LEFT JOIN truck_trailer tr
	ON ts.trailer_id = tr.id
LEFT JOIN delivery_driver dd
	ON rt.driver_id = dd.id
LEFT JOIN res_document_type drdt
	ON dd.document_type_id = drdt.id

JOIN ir_attachment iat
	ON iat.id = 3981
LEFT JOIN ir_config_parameter icp
	ON icp.key = 'filestore.path'

WHERE $X{IN, sp.id, IDS}

ORDER BY sp.name ASC]]>
	</queryString>
