	<parameter name="IDS" class="java.lang.Integer" isForPrompting="false"/>
	<queryString>
		<![CDATA[SELECT  aic.number AS "Check_number",
        aic.issue_date AS "Check_issue_date",
        aic.payment_date AS "Check_payment_date",
        rb.name AS "Check_bank_name",
        aic.amount AS "Check_amount"


FROM account_payment_order apo
	JOIN account_issued_check aic ON aic.payment_order_id = apo.id
	JOIN res_bank rb ON rb.id = aic.bank_id

WHERE apo.id IN ($P{IDS})]]>
	</queryString>
